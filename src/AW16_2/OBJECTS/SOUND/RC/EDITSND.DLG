#ifndef RCINVOKED
#define RCINVOKED

#include <windows.h>
#include <bwcc.h>

#include "..\soundres.h"

ICON_SOUND    ICON "sound.ico"
BMP_N_STOP    BITMAP stop_n.bmp
BMP_P_STOP    BITMAP stop_p.bmp
BMP_F_STOP    BITMAP stop_f.bmp
BMP_N_PAUSE   BITMAP pause_n.bmp
BMP_P_PAUSE   BITMAP pause_p.bmp
