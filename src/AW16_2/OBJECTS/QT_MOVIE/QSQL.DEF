LIBRARY         ALM_QSQL
DESCRIPTION     'Novell AppWare Loadable Module: ODBC SQL Objects'
EXETYPE         WINDOWS

CODE            LOADONCALL MOVEABLE DISCARDABLE
DATA            PRELOAD MOVEABLE SINGLE
SEGMENTS        _TEXT PRELOAD

HEAPSIZE        4096

EXPORTS         WEP                             @1              RESIDENTNAME
                GETSERVERSION                   @2

                ; /* ---- ODBC Connection object ------------------------- */
                OQCNCADMINISTRATIONROUTINE      @11
                OQCNCOBJECTDIALOG               @12

                OQCNCSTARTUPMETH                @18
                OQCNCSHUTDOWNMETH               @19

                ; /* ---- The functions ---- */
                OQCNCCOMMITFUNC                 @101
                OQCNCCONNECTFUNC                @102
                OQCNCDISCONNECTFUNC             @103
                OQCNCGETERRORFUNC               @104
                OQCNCROLLBACKFUNC               @105
                OQCNCSTATUSFUNC                 @106

                ; /* ---- ODBC SQL object -------------------------------- */
                OQSQLADMINISTRATIONROUTINE      @21
                OQSQLOBJECTDIALOG               @22
                OQSQLVARSDIALOG                 @23
                OQSQLOPTIONSDIALOG              @24

                OQSQLSTARTUPMETH                @28
                OQSQLSHUTDOWNMETH               @29

                ; /* ---- The functions ---- */
                OQSQLAPPENDTOTABLEFUNC          @201
                OQSQLDESCRIBETABLEFUNC          @202
                OQSQLEXECUTEFUNC                @203
                OQSQLFETCHFUNC                  @204
                OQSQLFETCHTOTABLEFUNC           @205
                OQSQLFETCHTOTEXTFUNC            @206
                OQSQLFLUSHFUNC                  @207
                OQSQLGETERRORFUNC               @208
                OQSQLGETTABLESFUNC              @209
                OQSQLGETVARIABLESFUNC           @210

                ; /* ---- The methods ---- */
                OQSQLDVIEWCHANGEDMETH           @251
                OQSQLDVIEWGETDATAMETH           @252
                OQSQLDVIEWGETROWCOUNTMETH       @253
                OQSQLDVIEWSETMETH               @254
