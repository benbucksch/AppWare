LIBRARY         AW32SQLD
DESCRIPTION     'Novell AppWare Loadable Module: SequeLink SQL Objects'
EXETYPE         WINDOWS

CODE            LOADONCALL MOVEABLE DISCARDABLE
DATA            PRELOAD MOVEABLE SINGLE
SEGMENTS        _TEXT PRELOAD

HEAPSIZE        4096

EXPORTS         DllEntryPoint                   @1
		OSqldGetVersion                 @2

		; /* ---- SequeLink SQL object -------------------------- */
		OSqldAdministrationRoutine      @11
		OSqldObjectDialog               @12
		OSqldConnSetsDialog             @13
		OSqldVarsDialog                 @14
		OSqldOptionsDialog              @15
		OSqldSubstrDialog               @16

		OSqldStartupMeth                @18
		OSqldShutdownMeth               @19

		; /* ---- The functions ---- */
		OSqldBuildQueryFunc             @101
		OSqldCommitFunc                 @102
		OSqldConnectFunc                @103
		OSqldDescribeTableFunc          @104
		OSqldDisconnectFunc             @105
		OSqldDisposeQueryFunc           @106
		OSqldExecuteQueryFunc           @107
		OSqldFetchFunc                  @108
		OSqldFetchToTableFunc           @109
		OSqldFetchToTextFunc            @110
		OSqldGetErrorFunc               @111
		OSqldGetStatusFunc              @112
		OSqldGetVariablesFunc           @113
		OSqldLogonFunc                  @114
		OSqldLogoffFunc                 @115
		OSqldRollbackFunc               @116
		OSqldStopFetchFunc              @117
