		OQsqlObjectDialog               @22
		OQsqlVarsDialog                 @23
		OQsqlOptionsDialog              @24

		OQsqlStartupMeth                @28
		OQsqlShutdownMeth               @29

		; /* ---- The functions ---- */
		OQsqlAppendToTableFunc          @201
		OQsqlDescribeTableFunc          @202
		OQsqlExecuteFunc                @203
		OQsqlFetchFunc                  @204
		OQsqlFetchToTableFunc           @205
